{
  "name": "react-ds-videoplayer",
  "version": "0.1.0",
  "private": false,
  "description": "A lightweight React video player with a built-in playlist.",
  "type": "module",
  "main": "./dist/react-ds-videoplayer.cjs.js",
  "module": "./dist/react-ds-videoplayer.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/react-ds-videoplayer.es.js",
      "require": "./dist/react-ds-videoplayer.cjs.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc -b"
  },
  "keywords": [
    "react",
    "video",
    "player",
    "playlist",
    "component"
  ],
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {},
  "devDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
